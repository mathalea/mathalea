{
  "name": "mathalea",
  "version": "2.8.2",
  "description": "Générateur d'exercices de mathématiques",
  "main": "mathalea.js",
  "scripts": {
    "build": "npm run build:prod",
    "build:dicos": "node tasks/buildDicos.js",
    "build:doc": "node tasks/buildDoc.js",
    "build:dev": "webpack --mode=development",
    "build:prod": "npm run build:dicos && webpack --mode=production && npm run doc",
    "doc": "node tasks/buildDoc.js && node tasks/showDoc.js",
    "doc:show": "node tasks/showDoc.js",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prepare": "husky install",
    "start": "webpack serve",
    "test": "node testsBrowser/start.js",
    "test:all": "node testsBrowser/start.js --task runAll --verbose",
    "test:list": "node testsBrowser/start.js --list",
    "watch": "webpack --watch",
    "release": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major"
  },
  "dependencies": {
    "@bugsnag/js": "^7.16.7",
    "@cortex-js/compute-engine": "^0.6.0",
    "@svgdotjs/svg.js": "^3.1.2",
    "algebrite": "^1.4.0",
    "clipboard": "^2.0.11",
    "codemirror": "^5.65.5",
    "core-js": "^3.22.8",
    "decimal.js": "^10.3.1",
    "earcut": "^2.2.3",
    "instrumenpoche": "git+https://git.sesamath.net/sesamath/instrumenpoche.git",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.13.1",
    "katex": "^0.13.24",
    "loadjs": "^4.2.0",
    "mathjs": "^9.5.2",
    "mathlive": "^0.69.11",
    "mathsteps": "github:mathalea/mathsteps",
    "qrcode": "^1.5.0",
    "regenerator-runtime": "^0.13.9",
    "seedrandom": "^3.0.5",
    "sval": "^0.4.8",
    "ts-loader": "^9.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/eslint-parser": "^7.18.2",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-modules": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-loader": "^8.2.5",
    "browserslist": "^4.20.3",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.7",
    "datatables.net": "^1.12.1",
    "datatables.net-dt": "^1.12.1",
    "docdash": "^1.2.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^6.0.0",
    "jsdoc": "^3.6.10",
    "jsdoc-plugin-typescript": "^2.0.7",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.6.2",
    "minimist": "^1.2.6",
    "playwright": "^1.22.2",
    "sesajs-date": "git+https://git.sesamath.net/sesamath/sesajs-date.git",
    "standard-version": "^9.5.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.3.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.7.3",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mathalea/mathalea.git"
  },
  "keywords": [
    "math",
    "exercise",
    "javascript"
  ],
  "author": "Rémi Angot <contact@coopmaths.fr>",
  "contributors": [
    "Rémi Angot",
    "Jean-Claude Lhote",
    "Sébastien Lozano",
    "Stéphane Guyon",
    "Gaëlle Morvan",
    "Mireille Gain",
    "Erwan Duplessy",
    "Cédric Grolleau",
    "Matthieu Devillers",
    "Liouba Leroux",
    "Daniel Caillibaud",
    "Guillaume Valmont",
    "Gilles Mora",
    "Laurence Candille",
    "Éric Schrafstetter",
    "Éric Elter",
    "Frédéric Piou",
    "Florence Tapiero",
    "Jean-Léon Henry",
    "Mathieu Degrange",
    "Loïc Geeraerts"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/mathalea/mathalea/issues"
  },
  "homepage": "https://github.com/mathalea/mathalea",
  "babel": {
    "plugins": [
      "@babel/syntax-dynamic-import"
    ],
    "presets": [
      [
        "@babel/preset-modules"
      ]
    ],
    "sourceType": "unambiguous"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "standard"
    ],
    "parser": "@babel/eslint-parser",
    "parserOptions": {
      "requireConfigFile": false
    },
    "overrides": [
      {
        "files": "test/**/*.js",
        "rules": {
          "no-unused-expressions": "off"
        },
        "env": {
          "mocha": true
        }
      },
      {
        "files": "testsBrowser/**/*.js",
        "rules": {
          "no-unused-expressions": "off"
        },
        "env": {
          "mocha": true
        },
        "globals": {
          "$": true,
          "browser": true,
          "expect": true
        }
      },
      {
        "files": "**/*.ts",
        "excludedFiles": "**/*.js",
        "parser": "@typescript-eslint/parser",
        "plugins": [
          "@typescript-eslint"
        ]
      }
    ]
  }
}