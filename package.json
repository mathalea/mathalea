{
  "name": "mathalea",
  "version": "2.5.0",
  "description": "Générateur d'exercices de mathématiques",
  "main": "mathalea.js",
  "scripts": {
    "doc": "jsdoc -c jsdoc/exemple.json",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "build": "npm run build:prod",
    "build:dicos": "node tasks/buildDicos.js",
    "build:dev": "webpack --mode=development",
    "build:prod": "npm run build:dicos && webpack --mode=production && npm run doc",
    "prepare": "husky install",
    "start": "webpack serve",
    "test": "node testsBrowser/start.js",
    "test:all": "node testsBrowser/start.js --task runAll --verbose",
    "test:list": "node testsBrowser/start.js --list",
    "watch": "webpack --watch"
  },
  "dependencies": {
    "algebrite": "^1.4.0",
    "clipboard": "^2.0.8",
    "codemirror": "^5.61.0",
    "core-js": "^3.11.0",
    "instrumenpoche": "git+https://framagit.org/Sesamath/instrumenpoche.git",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "katex": "^0.13.3",
    "mathjs": "^9.3.2",
    "qrcode": "^1.4.4",
    "regenerator-runtime": "^0.13.7",
    "seedrandom": "^3.0.5",
    "sval": "^0.4.8",
    "svg.js": "^2.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.15",
    "babel-loader": "^8.2.2",
    "browserslist": "^4.16.5",
    "copy-webpack-plugin": "^8.1.1",
    "css-loader": "^5.2.4",
    "datatables.net": "^1.10.24",
    "datatables.net-dt": "^1.10.24",
    "docdash": "^1.2.0",
    "eslint": "^7.25.0",
    "eslint-config-standard": "16.0.2",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "esm": "^3.2.25",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "jsdoc": "^3.6.6",
    "mini-css-extract-plugin": "^1.5.0",
    "minimist": "^1.2.5",
    "playwright": "^1.10.0",
    "sass": "^1.32.11",
    "sass-loader": "^11.0.1",
    "sesajs-date": "git+https://framagit.org/Sesamath/sesajs-date.git#main",
    "style-loader": "^2.0.0",
    "webpack": "^5.36.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/remiangot/MathALEA.git"
  },
  "keywords": [
    "math",
    "exercise",
    "javascript"
  ],
  "author": "Rémi Angot <contact@coopmaths.fr>",
  "contributors": [
    "Rémi Angot",
    "Jean-Claude Lhote",
    "Sébastien Lozano",
    "Stéphane Guyon",
    "Gaëlle Morvan",
    "Mireille Gain",
    "Erwan Duplessy",
    "Cédric Grolleau",
    "Matthieu Devillers",
    "Liouba Leroux"
  ],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/remiangot/MathALEA/issues"
  },
  "homepage": "https://github.com/remiangot/MathALEA#readme",
  "babel": {
    "plugins": [
      "syntax-dynamic-import"
    ],
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": {
            "version": "3.8",
            "proposals": true
          }
        }
      ]
    ],
    "sourceType": "unambiguous"
  },
  "browserslist": [
    "supports es6-module-dynamic-import"
  ],
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "standard"
    ]
  }
}
